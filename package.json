{
  "name": "earthworm",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "prisma generate && prisma migrate deploy && next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "vitest run",
    "postinstall": "prisma generate",
    "studio:dev": "dotenv -e .env.local -- npx prisma studio",
    "studio:prod": "npx prisma studio",
    "db:init": "dotenv -e .env.local -- npx prisma migrate dev && cd ./scripts && node createCourses.js && NODE_ENV=dev node uploadCourseData.js",
    "db:start": "docker-compose up"
  },
  "simple-git-hooks": {
    "pre-commit": "npm exec lint-staged"
  },
  "lint-staged": {
    "src/**/*.ts?(x)": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "@types/node": "20.6.3",
    "@types/react": "18.2.22",
    "@types/react-dom": "18.2.7",
    "@uidotdev/usehooks": "^2.4.0",
    "autoprefixer": "10.4.16",
    "axios": "^1.5.1",
    "eslint-config-next": "13.5.2",
    "next": "13.5.2",
    "next-themes": "^0.2.1",
    "postcss": "8.4.30",
    "prisma": "^5.4.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.2.4",
    "tailwindcss": "3.3.3",
    "typescript": "5.2.2",
    "zustand": "^4.4.2"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "eslint": "8.49.0",
    "eslint-plugin-prettier": "^5.0.1",
    "lint-staged": "^15.0.2",
    "pdf-parse": "^1.1.1",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "vitest": "^0.34.6"
  }
}
